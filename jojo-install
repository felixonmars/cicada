#!/usr/bin/env jojo

by another repl
by ungetc

(: (defun module-file->prefix-string (: file-name -> string)
     ))

(: (run (string "rsync -av --exclude='.git' . ~/.jojo/module/")
        (string "module.jo") module-file->prefix-string
        (int 2) n-command/run))

(run (int 66) byte/unread
     read/byte dup byte/print dot
     newline
     bye)
